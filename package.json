{
  "name": "stable-bridge",
  "version": "1.0.0",
  "main": "build",
  "scripts": {
    "build": "hardhat compile",
    "export-abi": "hardhat export-abi",
    "test": "hardhat test",
    "test-with-gas-usage": "NODE_GAS=true hardhat test",
    "test-fuzzy": "hardhat test test-fuzzy/pool.test.ts test-fuzzy/pool-balance.test.ts",
    "test-foundry": "forge test",
    "test-foundry:full": "forge test -vvvvv",
    "format": "npx prettier 'contracts/**/*.sol' 'hardhat.config.ts' 'test/**/*.ts' --write",
    "format:check": "npx prettier 'contracts/**/*.sol' 'hardhat.config.ts' 'test/**/*.ts' --check",
    "lint": "eslint \"test/**/*.ts\" --fix && solhint --fix 'contracts/**/*.sol'",
    "lint:check": "eslint \"test/**/*.ts\" && solhint 'contracts/**/*.sol'",
    "slither:all": "npm run slither:install && source ./venv/bin/activate && slither --config-file ./slither.config.json .",
    "slither:docker": "docker run -t -v .:/home/ethsec/evm trailofbits/eth-security-toolbox -c \"slither --config-file /home/ethsec/evm/slither.config.json /home/ethsec/evm\"",
    "python-env:install": "test -n \"$VIRTUAL_ENV\" || test -f ./venv/bin/activate || python3 -m venv venv",
    "slither:install": "npm run python-env:install && source ./venv/bin/activate && (command -v slither || pip3 install slither-analyzer)",
    "spell:check": "cspell \"{*.md,.github/*.md,contracts/**/*.sol,scripts/**/*.ts}\""
  },
  "dependencies": {
